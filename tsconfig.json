{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@pages/*": ["pages/*"],
      "@entity/*": ["entity/*"],
      "@utility/*": ["utility/*"]
    },
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "sourceMap": true,
    "outDir": "../tests-out",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
